<script>
jQuery(function(){
	'use strict';
	jQuery("#post-519").find("select[name=estado]").unbind("change").on("change", function(e){
	
		var uf_sigla = jQuery(this).find(":selected").val();
		
		var municipio = "https://icase.sbot.itarget.com.br/api/municipio-socio?uf=" + uf_sigla + "&token=da39a3ee5e6b4b0d3255bfef95601890afd80709";
	 
		if(uf_sigla){
			
			jQuery("select[name=cidade]").html("<option value>Carregando... </option>");
			jQuery("select[name=cidade]").attr("disabled", true);
			jQuery("select[name=estado]").attr("disabled", true);
			jQuery(".btn-warning").attr("disabled", true);
			jQuery.get( municipio, function( data ) {

			jQuery("select[name=cidade]").empty();
			// FOI O XIKÃƒO QUE FALOU PRA COLOCAR ASSIM, PIOR SE FOSSE NA NOIX
			// data['data'].sort((a, b) => a.descricao_sem_acento.localeCompare(b.descricao_sem_acento));

			if (data['data'].length > 0){
				var option = '<option value="">Município</option>';
				for ( var x=0; x < data['data'].length; x++ )
				{
				let dataPartic = data['data'][x];
				// console.log(dataPartic);
				option += '<option value="'+dataPartic['descricao']+'" class="filtro_estado filtro_' + uf_sigla + '">'+dataPartic['descricao']+'</option>';
				}

				jQuery("select[name=cidade]").attr("disabled", false);
				jQuery("select[name=estado]").attr("disabled", false);
				jQuery('#btn_encontre_ortopedista').attr("disabled", false);
			}

			jQuery("select[name=cidade]").html(option);
			});
			
		} else {
			jQuery("select[name=cidade]").empty();
			jQuery("select[name=cidade]").html("<option value>Cidade</option>"); 
		}
	}); 
}); 
</script>
