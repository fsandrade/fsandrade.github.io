						jQuery("a[title=informacoes-financeira],a[title=link-ead],a[title=link-rima],a[title=link-rede-parcerias],a[title=link-menu-atualizacao],a[title=link-menu-consensos-orto],a[title=link-menu-consensos-infecto],a[title=link-menu-manual-orto],a[title=link-menu-manual-trauma],a[title=link-menu-trabalho-cientifico]").unbind("click touch").on("click touch",function(e){
